package com.newtriks.model{	import com.newtriks.model.vo.BookmarkVO;		import flash.events.Event;	import flash.net.URLLoader;	import flash.net.URLRequest;		import mx.collections.ArrayCollection;		import org.puremvc.interfaces.IProxy;	import org.puremvc.patterns.proxy.Proxy;		public class XMLProxy extends Proxy implements IProxy	{		public static const NAME:String = "XMLProxy";				// Ammend to your xml file on local system		private var url:String = "../assets/xml/bookmarks.xml";				private var urlLoader:URLLoader;		private var urlRequest:URLRequest;				public function XMLProxy()		{			super( NAME, new ArrayCollection );						getXML();		}				// Return data property cast to proper type		public function get bookmarks():ArrayCollection		{			return data as ArrayCollection;		}				public function getXML():void		{				urlRequest = new URLRequest();			urlRequest.url = url;						urlLoader = new URLLoader();			urlLoader.addEventListener( Event.COMPLETE, onComplete );						urlLoader.load( urlRequest );		}				private function onComplete( event:Event ):void		{			var resultXML:XML = XML( urlLoader.data );					    for each(var item:XML in resultXML..bookmark)		    {		    			      	var bm:BookmarkVO = new BookmarkVO();		        			        bm.title = item.title;		        bm.category = item.category;		        bm.url = item.url;		        			        bookmarks.addItem( bm );		    }  		}	}}