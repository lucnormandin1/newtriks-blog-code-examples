package com.newtriks.controller{	import org.puremvc.interfaces.ICommand;	import org.puremvc.patterns.command.SimpleCommand;	import org.puremvc.interfaces.INotification;		import com.newtriks.ApplicationFacade;	import com.newtriks.model.*;	import com.newtriks.view.*;		public class StartupCommand extends SimpleCommand implements ICommand	{		/**		 * Register the Proxies and Mediators.		 * 		 * Get the View Components for the Mediators from the app,		 * which passed a reference to itself on the notification.		 */		 override public function execute( note:INotification ):void		 {		 			 	var app:index = note.getBody() as index;		 			 	facade.registerProxy( new XMLProxy() );						facade.registerMediator( new BookmarkGridMediator( app.bookmarkGrid ) );		 }	}}