package com.newtriks.view{	import com.newtriks.model.XMLProxy;	import com.newtriks.view.components.BookmarkGrid;		import org.puremvc.interfaces.IMediator;	import org.puremvc.patterns.mediator.Mediator;	public class BookmarkGridMediator extends Mediator implements IMediator	{		public static const NAME:String = 'BookmarkGridMediator';				private var xmlProxy:XMLProxy;				public function BookmarkGridMediator( viewComponent:Object )		{			super( viewComponent );						xmlProxy = facade.retrieveProxy( XMLProxy.NAME ) as XMLProxy;			bookMarkGrid.bookmarks = xmlProxy.bookmarks;					}				override public function getMediatorName():String		{			return NAME;		}				public function get bookMarkGrid():BookmarkGrid		{			return viewComponent as BookmarkGrid;		}			}}